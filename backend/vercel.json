{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles":[
                    "/dist/schemas/**/*.graphql"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "env": {
        "DB_NAME": "@db_name",
        "MONGO_URI": "@mongo_uri"
    }
}